{
  "name": "midnighter",
  "description": "A minimalistic feathers and vue app for deployment to heroku",
  "version": "0.1.1",
  "homepage": "",
  "main": "server",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Ahmed Nzibo",
    "email": "a.nzibo@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kigenofkenya/midnighter"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/"
  },
  "engines": {
    "node": "^9.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production HOST='penny.local' PORT=3080 node server/index.js",
    "dev": "node server/index.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js --progress --hide-modules",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "unit": "karma start ./test/unit/karma.conf.js --single-run",
    "test": "npm run eslint && npm run mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.js",
    "mocha:feathers": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.0",
    "@feathersjs/authentication-jwt": "^1.0.1",
    "@feathersjs/authentication-local": "^1.0.2",
    "@feathersjs/authentication-oauth2": "^1.0.3",
    "@feathersjs/configuration": "^1.0.2",
    "@feathersjs/errors": "^3.2.2",
    "@feathersjs/express": "^1.1.2",
    "@feathersjs/feathers": "^3.1.0",
    "axios": "^0.17.1",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.14.0",
    "feathers-memory": "^2.1.0",
    "feathers-nedb": "^3.0.0",
    "helmet": "^3.10.0",
    "lru-cache": "^4.0.1",
    "nedb": "^1.8.0",
    "nextgen-events": "^0.10.2",
    "passport-auth0": "^0.6.1",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.13",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.13",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "winston": "^2.4.0",
    "ws": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-2": "^6.13.0",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.28.0",
    "karma": "^2.0.0",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon-chai": "^1.2.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^2.0.2",
    "mocha": "^5.0.0",
    "phantomjs-prebuilt": "^2.1.10",
